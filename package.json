{
  "name": "trading-view-screener-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "ts-node-esm src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec ts-node-esm src/main.ts",
    "test": "jest --coverage --reporters='default'",
    "build": "tsc"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/koa": "^1.1.0",
    "@discordx/pagination": "^3.0.0",
    "@koa/router": "^12.0.0",
    "@types/node-fetch": "^2.6.2",
    "discord.js": "^14.2.0",
    "discordx": "^10.0.0",
    "koa": "^2.13.4",
    "log4js": "^6.6.1",
    "node-fetch": "^2.6.7",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.7",
    "@types/koa": "^2.13.5",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^18.7.5",
    "@types/node-fetch": "^2.6.2",
    "babel-jest": "^28.1.3",
    "install": "^0.13.0",
    "jest": "^28.1.3",
    "log4js": "^6.6.1",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.19",
    "npm": "^8.17.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "@screener-utils/*": "<rootDir>/src/utils/",
      "@screener-services/*": "<rootDir>/src/services/",
      "@screener-types/*": "<rootDir>/src/types/",
      "@screener-config/*": "<rootDir>/src/config/",
      "@screener-managers/*": "<rootDir>/src/managers/"
    },
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/unit/**/*"
    ]
  }
}
